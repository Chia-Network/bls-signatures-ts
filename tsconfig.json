{
    "compilerOptions": {
        "downlevelIteration": true,
        "esModuleInterop": true,
        "inlineSourceMap": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitOverride": true,
        "isolatedModules": true,
        "strict": true,
        "moduleResolution": "node",
        "target": "ESNext",
        "lib": ["ESNext", "DOM"]
    }
}
